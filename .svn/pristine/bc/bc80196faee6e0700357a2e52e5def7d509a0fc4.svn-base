#tree .fileO {height:20px;line-height:20px;background:url('./../images/icon_o.jpg') 5px 50% no-repeat; padding-left:25px; cursor:pointer; cursor:pointer; background-color:#ffffff; border: dashed 1px #ffffff;}
#tree .fileP {height:20px;line-height:20px;background:url('./../images/icon_p.jpg') 5px 50% no-repeat; padding-left:25px; cursor:pointer; cursor:pointer; background-color:#ffffff; border: dashed 1px #ffffff;}
#tree .fileP2 {height:20px;line-height:20px;background:url('./../images/icon_p2.jpg') 5px 50% no-repeat; padding-left:25px; cursor:pointer; cursor:pointer; background-color:#ffffff; border: dashed 1px #ffffff;}
#tree .fileP3 {height:20px;line-height:20px;background:url('./../images/icon_p3.jpg') 5px 50% no-repeat; padding-left:25px; cursor:pointer; cursor:pointer; background-color:#ffffff; border: dashed 1px #ffffff;}
